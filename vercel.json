{
  "version": 2,
  "builds": [
    { "src": "api/addUser.js", "use": "@vercel/node" },
    { "src": "api/getUsers.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/addUser",
      "methods": ["POST"],
      "dest": "/api/addUser.js"
    },
    {
      "src": "/api/getUsers",
      "methods": ["GET"],
      "dest": "/api/getUsers.js"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "/intro.html"
    }
  ]
}
